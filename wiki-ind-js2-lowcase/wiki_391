biro a_dijabat oleh sersan_mayor_soehardjo
biro b_dijabat oleh sersan_mayor_kurtubi
biro c_dijabat oleh sersan_mayor_r.t . sulaeman
biro d_dijabat oleh sersan_mayor_gumelar
